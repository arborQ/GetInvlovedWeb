    "cors": "^2.7.1",
    "debug": "~2.2.0",
    "es6-promise": "^3.1.2",
    "eslint": "^2.11.1",
    "express": "~4.13.1",
    "express-jwt": "^3.4.0",
    "flat": "^2.0.0",
    "fs": "0.0.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
